{
  "name": "mp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "docker-build": "npm run build && docker-compose build",
    "docker-cleanup": "npm run docker-rm-dangling-imgs && npm run docker-rm-old-containers",
    "docker-rm-dangling-imgs": "docker images -f dangling=true -q | xargs docker rmi --force",
    "docker-rm-old-containers": "docker ps -aq -f status=exited | xargs docker rm",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.4",
    "@angular/common": "^4.3.4",
    "@angular/compiler": "^4.3.4",
    "@angular/core": "^4.3.4",
    "@angular/forms": "^4.3.4",
    "@angular/http": "^4.3.4",
    "@angular/platform-browser": "^4.3.4",
    "@angular/platform-browser-dynamic": "^4.3.4",
    "@angular/router": "^4.3.4",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "intl": "^1.2.5",
    "ng2-bootstrap": "^1.6.3",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.0",
    "@angular/compiler-cli": "^4.3.4",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^8.0.20",
    "codelyzer": "^3.1.2",
    "jasmine-core": "^2.7.0",
    "jasmine-spec-reporter": "^4.2.0",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31",
    "protractor": "~5.1.0",
    "ts-node": "^3.0.2",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  }
}
